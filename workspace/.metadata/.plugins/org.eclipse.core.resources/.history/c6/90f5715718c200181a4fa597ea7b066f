package com.bit.day15;

class MyArray{
	private int[] target;
	
	public MyArray() {
		target=new int[0];
	}
	
	//입력
	public void add(int a){
		int[] old=target;
		target=new int[target.length+1];
		for(int i=0; i<old.length; i++){
			target[i]=old[i];
		}
		target[target.length-1]=a;
	}
	
	// 사이즈
	public int size(){
		return target.length;
	}
	
	//출력
	public int get(int idx){
		return target[idx];
	}
	
}

public class Ex06 {

	public static void main(String[] args) {
		MyArray arr1=new MyArray();
		arr1.add(1111);
		arr1.add(2222);
		arr1.add(3333);
		arr1.add(4444);
		arr1.add(1111);
		arr1.add(2222);
		arr1.add(3333);
		arr1.add(4444);
		arr1.add(1111);
		arr1.add(2222);
		arr1.add(3333);
		arr1.add(4444);
		
		for(int i=0; i<arr1.size(); i++){
			System.out.println(arr1.get(i));
		}
		
	}

}




















