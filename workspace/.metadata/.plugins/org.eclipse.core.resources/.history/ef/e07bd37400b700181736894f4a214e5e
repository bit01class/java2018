package com.bit.day10;

public class Ex09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Car car1=new Car("마티즈");
		car1.speedUp();
		car1.speedUp();
		car1.speedUp();
	}

}


class Car{
	int speed;
	String name;
	
	public Car(String msg) {
		name=msg;
	}
	
	void speedUp(){
		speed+=10;
		System.out.println(name+"의 현재속도:"+speed+"km");
	}
}