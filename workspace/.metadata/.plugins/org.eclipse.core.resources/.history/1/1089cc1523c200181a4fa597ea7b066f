package com.bit.day15;

class Node{
	int val;
	Node node;
}

class MyLink{
	Node start;
	int cnt;
	
	public int size(){
		return cnt;
	}
	
	public void add(int a){
		if(cnt==0){
			start= new Node();
			start.val=a;
			cnt++;
		}else{
			start.node=new Node();
			start.node.val=a;
			cnt++;
			
			start.node.node=new Node();
			start.node.node.val=a;
			cnt++;
		}
	}
}

public class Ex08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}













