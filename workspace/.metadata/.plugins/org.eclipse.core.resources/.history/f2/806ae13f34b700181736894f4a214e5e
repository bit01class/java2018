package com.bit.day10;

class Ball{
	private int num;
	
	public Ball(int a) {
		num=a;
	}
	public int show(){
		return num;
	}
}

public class Ex17 {

	public static void main(String[] args) {
		int su=6;
		Ball[] balls=new Ball[su];
		for(int i=0; i<balls.length; i++){
			balls[i]=new Ball(i+1);
		}
		Ball box;
		for(int i=0; i<10000; i++){
			int ran=(int)(Math.random()*su);
			box=balls[0];
			balls[0]=balls[ran];
			balls[ran]=box;
		}
		
		for(int i=0; i<6; i++){
			System.out.println(balls[i].show());
		}
		
	}

}












