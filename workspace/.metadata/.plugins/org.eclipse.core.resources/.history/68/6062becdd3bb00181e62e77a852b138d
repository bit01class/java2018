package com.bit.day13;

public class Ex10 {

	public static void main(String[] args) {
		try{
			int result = 10/0;
		}catch(ArithmeticException err){
			System.out.println("에러회피");
			System.out.println(err);
			System.out.println(err.toString());
			System.out.println(err.getMessage());
			err.printStackTrace();
		}
	}

}
