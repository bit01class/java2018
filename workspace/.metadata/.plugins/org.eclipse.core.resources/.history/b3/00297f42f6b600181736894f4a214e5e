package com.bit.day10;

public class Ex03 {

	public static void main(String[] args) {
		// 알파벳 A-Z까지를 배열에 넣고 출력하라
		int cnt;
		cnt=(int)('Z'-'A')+1;
		char[] arr=new char[cnt];
		for(int i=0; i<cnt; i++){
			arr[i]=(char)('A'+i);
		}
		for(int i=0; i<cnt; i++){
			System.out.print(arr[i]+" ");
		}

	}

}
