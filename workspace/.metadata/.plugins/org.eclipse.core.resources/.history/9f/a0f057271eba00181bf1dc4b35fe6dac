package com.bit.day11;

class Car{
	// 속성요소
	// 명사
	String company;
	String model;
	String color;
	int year;
	int speed;	
	
	public Car() {
	}
	
	// 기능요소
	// ~ 한다(동사)
	void speedUp(int speed){
		this.speed+=speed;
	}
	void start(){
		System.out.println("달린다");
	}
	
	void stop(){
		
	}
}

public class Ex01 {
	int num;
	Car car;

	public static void main(String[] args) {
		// 필드
		new Car();

	}

}
