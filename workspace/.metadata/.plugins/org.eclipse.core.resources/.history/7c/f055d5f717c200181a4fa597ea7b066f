package com.bit.day15;

class MyArray{
	int[] target;
	
	public MyArray() {
		target=new int[0];
	}
	
	//입력
	public void add(int a){
		int[] old=target;
		target=new int[target.length+1];
		for(int i=0; i<old.length; i++){
			target[i]=old[i];
		}
		target[target.length-1]=a;
	}
	
	//출력
	public int get(int idx){
		return target[idx];
	}
	
}

public class Ex06 {

	public static void main(String[] args) {

	}

}
