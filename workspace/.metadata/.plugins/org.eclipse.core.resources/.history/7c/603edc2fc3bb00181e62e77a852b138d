package com.bit.day13;

import java.io.InputStream;
/*
 * 
 * 123456-1234567
 * '1''2''3''4''5''6''-''1''2''3''4''5''6''7'
 * ÀÏÀÌ»ï»ç¿ÀÀ°-ÀÏÀÌ»ï»ç¿ÀÀ°Ä¥
 * 
*/
public class Ex07 {

	public static void main(String[] args) {
		//ÇÑ±ÛÃ³¸®(ÇÑ±Û¸¸)
		char[] hangle={'¿µ','ÀÏ','ÀÌ','»ï','»ç','¿À','À°','Ä¥','ÆÈ','±¸'};
		
		InputStream is = System.in;
		java.util.Scanner sc = new java.util.Scanner(is);
		String input=null;
		
		System.out.println("-------------------------------------\nÁÖ¹Î¹øÈ£ È®ÀÎ\n----------------------------------");
		boolean result=true;
		while(true){
			System.out.print("ÁÖ¹Î¹øÈ£ ÀÔ·Â>");
			input=sc.nextLine();
			
//			for(int i=0; i<input.length(); i++){
//				if(Character.isDigit(input.charAt(i))){
//					
//					continue;
//				}else if(input.charAt(6)!='-'){
//					
//					continue;
//				}
//			}
			
			for(int i=0; i<input.length(); i++){
				if(i==6){
					System.out.print('-');
					continue;
					}
				char target=input.charAt(i);
				int idx=0;
				for(idx=0; idx<hangle.length; idx++){
					if(target==hangle[idx]){break;}
				}
				System.out.print(idx);
			}
		}
	}

}






























