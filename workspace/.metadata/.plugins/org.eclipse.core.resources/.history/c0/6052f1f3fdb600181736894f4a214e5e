package com.bit.day10;

public class Ex07 {

	public static void main(String[] args) {
		//자료형[] 배열명;
		int[] arr1;
		arr1=new int[]{1,2,3,4};
		char[] arr2;
		arr2=new char[]{'a','b','c'};
		double[] arr3;
		arr3=new double[]{1.0,1.1,1.2,1.3};
//		int[] arr1={1,2,3,4};
//		char[] arr2={'a','b','c'};
//		double[] arr3={1.0,1.1,1.2,1.3};
		byte[] arr4;
		arr4=new byte[]{1,2,3,4,5};
		long[] arr5;
		arr5=new long[]{1,2,3,4,5};
//		float[] arr6=new float[]{1.0,2.0,3.0,4.0};
	}

}
