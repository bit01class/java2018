package com.bit.day14;
/*
 * ------------------------------------
 * 계산기(ver 0.0.2)
 * ------------------------------------
 * 입력>1+1
 * 결과:2
 * 
 * 1+1=2
 * 2-1=1;
 * 2*1=2;
 * 3/2=1;
 * 3/2=1.xxx;
 * 4/2=2
 * 
*/
public class Ex09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		func01(3,2.14);
	}
	public static void func01(Number a, Number b){
		double c=(Double)a;
		double d=(Double)b;
		System.out.println(c/d);
	}

}












