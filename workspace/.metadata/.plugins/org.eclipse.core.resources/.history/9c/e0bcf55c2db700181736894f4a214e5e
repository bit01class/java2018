package com.bit.day10;

public class Ex15 {

	public static void main(String[] args) {
		// 중복 검출
		int[] arr = new int[4];
		
		arr[0]=random();
		arr[1]=random();
		while(arr[0]!=arr[1]){
			arr[1]=random();
		}
		arr[2]=random();
		while(arr[0]!=arr[2]){
			while(arr[1]!=arr[2]){
				arr[2]=random();
			}
		}

	}
	
	// 1~4
	public static int random(){
		return (int)(Math.random()*4)+1;
	}

}
