package com.bit.day10;

public class Ex14 {

	public static void main(String[] args) {
		// 오름차순
		int[] arr=new int[]{9,5,3};

		//arr[0]~[2]
		int temp=0;
		
		if(arr[0]>arr[1]){
			temp=arr[0];
			arr[0]=arr[1];
			arr[1]=temp;
		}
		if(arr[0]>arr[2]){
			temp=arr[0];
			arr[0]=arr[2];
			arr[2]=temp;
		}
	}

}
