package com.bit.day11;

class Car extends Object{
	// 속성요소
	// 명사
	String company;
	String model;
	String color;
	int year;
	int speed;	
	
	public Car() {
		model="티코";
	}
	
	// 기능요소
	// ~ 한다(동사)
	void speedUp(int speed){
		this.speed+=speed;
	}
	void start(){
		System.out.println("달린다");
	}
	
	void stop(){
		
	}
}

public class Ex01 extends Car{
	
	public Ex01() {
		System.out.println("ex01 객체 생성");
	}

	public static void main(String[] args) {

		Ex01 me= new Ex01();
		me.start();
		System.out.println(me.model);
		
	}

}
