package com.bit.day15;

import java.util.ArrayList;

public class Ex07 {

	public static void main(String[] args) {
		// ArrayList
		ArrayList list = new ArrayList();
		list.add(1111);
		list.add(2222);
		list.add(3.14);
		list.add('a');
		list.add(false);
		list.add("String");
		
		
		System.out.println(list.get(2));
		
	}

}
