package com.bit.day11;

class Total{
	int su;
	
	public Total(int su) {
		this.su=su;
	}
	
	void plus(int a){
		su+=a;
	}
	
	@Override
	public String toString(){
		return ""+su;
	}
}

public class Ex07 {

	public static void main(String[] args) {
		Total tot=new Total(100);
		tot.plus(10);
		tot.plus(20);
		System.out.println(tot);
	}

}
