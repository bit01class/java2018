package com.bit.day10;

public class Ex12 {

	public static void main(String[] args) {
		// 로또 번호 생성기
		// 방법3가지
		// 1 ~ 45 공 공추첨-
		// 랜덤
		// 중복검출
		// 오름차순
		int[] lotto = new int[45];
		
		for(int i=0; i<lotto.length; i++){
			// 0~ 44
			lotto[i]=(int)(Math.random()*44)+1;
		}
		for(int i=0; i<6; i++){
			System.out.print(lotto[i]+" ");
		}
	}

}
