package com.bit.day15;

interface inter01{
	// 오직 추상메서드와 상수형 변수만 갖음
	// 메서드 - abstract - 생략가능
	// 메서드 - 접근제한자는 오직 public - 생략가능
	// 변수 - final 생략가능
	final int su1=1111;
	int su2=2222;
	
	public abstract void func01();
	
	public void func02();
	
	void func03(); 
}

interface inter02 extends inter01{
	// interface들 끼리의 상속은 extends
	int su2=333;
	
	void func03();
	void func04();
}

interface inter03{
	void func04();
}


















