package com.bit.day10;

class Ball2{
	int num;
	boolean marking;
}

public class Ex18 {

	public static void main(String[] args) {
		Ball2[] balls=new Ball2[45];
		Ball2[] box=new Ball2[6];

		for(int i=0; i<balls.length; i++){
			balls[i]=new Ball2();
			balls[i].num=i+1;
			balls[i].marking=false;
		}
	}

}
